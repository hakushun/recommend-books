{
	"hosting": {
		"public": "out",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/login",
				"destination": "/login.html"
			},
			{
				"source": "/books",
				"destination": "/books.html"
			},
			{
				"source": "/book/[id]",
				"destination": "/book/[id].html"
			}
		]
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	}
}
